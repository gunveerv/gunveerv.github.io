{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "react/react-in-jsx-scope": "off",
        "react/jsx-uses-react": "off",
        "react/no-unescaped-entities": 0,
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "keyword-spacing": ["warn", { "before": true, "after": true }]
    }
}
